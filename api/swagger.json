{
  "swaggerDefinition": {
    "info": {
      "description": "This a sample swagger server",
      "title": "Swagger",
      "version": "1.0.0"
    },
    "host": "localhost:4000",
    "basePath": "/v1",
    "produces": [
      "application/json"
    ],
    "schemes": [
      "http"
    ],
    "securityDefinitions": {
      "JWT": {
        "type": "apiKey",
        "in": "header",
        "name": "Authorization",
        "description": "Bearer<token>"
      }
    }
  },
  "files": [
    "./routers/**/*.js",
    "./models/**/*.js"
  ]
}